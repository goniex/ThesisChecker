<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.thesischecker.controller.UserEntity" table="user" schema="" catalog="thesis">
        <id name="id">
            <column name="id" sql-type="int" not-null="true"/>
        </id>
        <property name="email">
            <column name="email" sql-type="varchar"/>
        </property>
        <property name="salt">
            <column name="salt" sql-type="varchar"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar"/>
        </property>
        <property name="role">
            <column name="role" sql-type="varchar"/>
        </property>
        <property name="token">
            <column name="token" sql-type="varchar"/>
        </property>
        <property name="active">
            <column name="active" sql-type="tinyint"/>
        </property>
        <property name="createdAt">
            <column name="created_at" sql-type="datetime" not-null="true"/>
        </property>
        <property name="updatedAt">
            <column name="updated_at" sql-type="datetime" not-null="true"/>
        </property>
        <property name="deletedAt">
            <column name="deleted_at" sql-type="datetime"/>
        </property>
        <many-to-one name="Analysis" class="com.thesischecker.controller.AnalysisEntity">
            <column name="id" not-null="true"/>
        </many-to-one>
        <many-to-one name="Updates" class="com.thesischecker.controller.UserUpdateEntity">
            <column name="id" not-null="true"/>
        </many-to-one>
        <many-to-one name="Resources" class="com.thesischecker.controller.ResourceEntity">
            <column name="id" not-null="true"/>
        </many-to-one>
        <one-to-one name="Profile" class="com.thesischecker.controller.UserProfileEntity"/>
    </class>
</hibernate-mapping>